{
  "system": "n64",
  "overlays": {
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-N64",
        "path": "retroarch/overlay/GameBezels/N64"
      },
      {
        "repo": "thebezelproject/bezelprojectsa-N64",
        "path": "retroarch/overlay/GameBezels/N64",
        "default": "retroarch/overlay/Nintendo-64.png"
      }
    ]
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "mupen64plus": {},
    "lr-mupen64plus": {"core_name": "mupen64plus", "library_name": "Mupen64Plus GLES3", "default": true},
    "lr-mupen64plus-next": {"core_name": "mupen64plus-next", "library_name": "Mupen64Plus-Next"},
    "lr-parallel-n64": {"core_name": "parallel-n64", "library_name": "ParaLLEl N64"}
  },
  "cheats": {
    "name": "Nintendo - Nintendo 64"
  },
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Nintendo - Nintendo 64 \\(BigEndian\\)"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$HOME/retrokit/cache/nointro/Nintendo - Nintendo 64 (BigEndian) (Parent-Clone).dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "download": "$HOME/RetroPie/roms/n64/.nointro/{machine}.zip",
          "target": "$HOME/RetroPie/roms/n64/.nointro/{machine}.z64",
          "install": {"action": "zip_extract", "file": ".*\\.z64"}
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "emulators": {
      "url": "file://$HOME/retrokit/config/systems/n64/emulators.tsv",
      "filter": true
    },
    "priority": [
      "USA",
      "World"
    ],
    "filters": {
      "flags": [
        "Europe",
        "USA",
        "World"
      ],
      "!flags": [
        "Beta",
        "Collection",
        "Demo",
        "Pirate",
        "Proto",
        "Unl",
        "Virtual Console"
      ],
      "!titles": [
        "# Collections",
        "Midway's Greatest Arcade Hits - Volume 1",
        "Namco Museum"
      ],
      "demo": [
        "1080 Snowboarding",
        "Carmageddon 64",
        "GoldenEye 007",
        "Mario Kart 64",
        "Rampage - World Tour",
        "Re-Volt"
      ]
    },
    "favorites": {
      "titles": [
      ]
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/n64"}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
