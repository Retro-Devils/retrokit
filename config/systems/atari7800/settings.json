{
  "system": "atari7800",
  "themes": {
    "bezel": "Atari7800"
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "lr-prosystem": {"core_name": "prosystem", "library_name": "ProSystem", "default": true}
  },
  "cheats": "Atari - 7800",
  "bios": {
    "url": "$BIOS_URL/Atari%20-%207800",
    "dir": "/home/pi/RetroPie/BIOS",
    "files": {
      "7800 BIOS (U).rom": "{url}/7800%20BIOS%20(U).rom"
    }
  },
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Atari - 7800"
        }
      },
      "resources": {
        "dat": {
          "source": "file:///home/pi/retrokit/cache/nointro/Atari - 7800.dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "/home/pi/RetroPie/roms/atari7800/.nointro/{machine}.zip"
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "priority": [
      "usa"
    ],
    "filters": {
      "flags": [
        "usa",
        "europe"
      ],
      "!flags": [
        "proto",
        "unl"
      ],
      "!keywords": [
        "bios",
        "development card",
        "test cartrige"
      ]
    },
    "files": {
      "machine": {"target": "{dir}/{machine}.zip"}
    },
    "dirs": [
      {"path": "/home/pi/RetroPie/roms/atari7800"}
    ]
  }
}
