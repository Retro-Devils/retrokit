{
  "system": "nes",
  "overlays": {
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-NES",
        "path": "retroarch/overlay/GameBezels/NES"
      },
      {
        "repo": "thebezelproject/bezelprojectsa-NES",
        "path": "retroarch/overlay/GameBezels/NES",
        "default": "retroarch/overlay/Nintendo-Entertainment-System.png"
      }
    ],
    "lightgun_border": {
      "offset_x": 232
    }
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "lr-fceumm": {"core_name": "fceumm", "library_name": "FCEUmm", "default": true}
  },
  "cheats": {
    "name": "Nintendo - Nintendo Entertainment System"
  },
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Nintendo - Nintendo Entertainment System.*\\[headered\\]"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$HOME/retrokit/cache/nointro/Nintendo - Nintendo Entertainment System (Parent-Clone).dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/nes/.nointro/{machine}.zip",
          "file_identifier": "name"
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "priority": [
      "USA",
      "World"
    ],
    "filters": {
      "flags": [
        "Europe",
        "USA",
        "World"
      ],
      "!flags": [
        "Aftermarket",
        "Alt",
        "Beta",
        "Collection",
        "Demo",
        "Kiosk",
        "Proto",
        "Sample",
        "Test",
        "Unl",
        "Virtual Console"
      ],
      "!keywords": [
        "Anniversary",
        "Cartridge",
        "Collector",
        "Gamecube",
        "Game Genie",
        "Namco Museum",
        "Super Mario Bros. +",
        "Teenage Mutant Hero"
      ],
      "!titles": [
        "# Board Games",
        "Anticipation",
        "Chessmaster, The",
        "Punch-Out!!",
        "# Casino",
        "Caesars Palace",
        "Casino Kid II",
        "Vegas Dream",
        "# Educational",
        "Color a Dinosaur",
        "Donkey Kong Jr. Math",
        "Fisher-Price - I Can Remember",
        "Mickey's Adventure in Numberland",
        "Mickey's Safari in Letterland",
        "Miracle Piano Teaching System, The",
        "Sesame Street",
        "Tiny Toon Adventures Cartoon Workshop",
        "Videomation"
      ],
      "!names": [
        "# Not available",
        "Alex DeMeo's Race America (USA)"
      ],
      "+names": [
        "Karateka (Japan)"
      ],
      "demo": [
        "Bubble Bobble"
      ]
    },
    "favorites": {
      "titles": [
      ]
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/nes"}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
