{
  "system": "nes",
  "overlays": {
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-NES",
        "path": "retroarch/overlay/GameBezels/NES"
      },
      {
        "repo": "thebezelproject/bezelprojectsa-NES",
        "path": "retroarch/overlay/GameBezels/NES",
        "default": "retroarch/overlay/Nintendo-Entertainment-System.png"
      }
    ],
    "lightgun_border": {
      "offset_x": 232
    }
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "lr-fceumm": {"core_name": "fceumm", "library_name": "FCEUmm", "default": true}
  },
  "cheats": "Nintendo - Nintendo Entertainment System",
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Nintendo - Nintendo Entertainment System.*\\[headered\\]"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$HOME/retrokit/cache/nointro/Nintendo - Nintendo Entertainment System (Parent-Clone).dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/nes/.nointro/{machine}.zip",
          "file_identifier": "name"
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "priority": [
      "USA",
      "World"
    ],
    "filters": {
      "flags": [
        "Europe",
        "USA",
        "World"
      ],
      "!flags": [
        "aftermarket",
        "alt",
        "beta",
        "collection",
        "demo",
        "kiosk",
        "proto",
        "sample",
        "test",
        "unl",
        "virtual console"
      ],
      "!keywords": [
        "anniversary",
        "cartridge",
        "collector",
        "gamecube",
        "game genie",
        "namco museum",
        "super mario bros. +",
        "teenage mutant hero"
      ],
      "!names": [
        "Alex DeMeo's Race America (USA)"
      ],
      "!titles": [
        "Chessmaster, The",
        "Punch-Out!!"
      ],
      "+names": [
        "Karateka (Japan)"
      ],
      "demo": [
        "Bubble Bobble"
      ]
    },
    "favorites": {
      "titles": [
        "Arkanoid",
        "Bubble Bobble",
        "Bubble Bobble Part 2",
        "California Games",
        "Contra",
        "Contra Force",
        "Double Dragon",
        "Double Dragon II - The Revenge",
        "Double Dragon III - The Sacred Stones",
        "Excitebike",
        "Duck Hunt",
        "Indiana Jones and the Last Crusade",
        "Indiana Jones and the Temple of Doom",
        "Karateka",
        "Little Mermaid, The",
        "Marble Madness",
        "Mike Tyson's Punch-Out!!",
        "Monster Truck Rally",
        "Ms. Pac-Man",
        "Pac-Man",
        "Popeye",
        "Rampage",
        "Paperboy",
        "R.C. Pro-Am",
        "R.C. Pro-Am II",
        "Simpsons, The - Bartman Meets Radioactive Man",
        "Simpsons, The - Bart vs. the Space Mutants",
        "Simpsons, The - Bart vs. the World",
        "Skate or Die",
        "Skate or Die 2 - The Search for Double Trouble",
        "Super Mario Bros.",
        "Super Mario Bros. 2",
        "Super Mario Bros. 3",
        "Teenage Mutant Ninja Turtles",
        "Teenage Mutant Ninja Turtles II - The Arcade Game",
        "Teenage Mutant Ninja Turtles III - The Manhattan Project",
        "Tetris",
        "Tetris 2",
        "Track & Field",
        "Track & Field II",
        "Winter Games",
        "World Games"
      ]
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/nes/-ALL-"},
      {
        "path": "$HOME/RetroPie/roms/nes",
        "filters": {"favorites": [true]}
      }
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
