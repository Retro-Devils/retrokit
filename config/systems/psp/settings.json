{
  "system": "psp",
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "ppsspp": {"default": true}
  },
  "romsets": [
    {
      "name": "redump",
      "protocol": "http",
      "url": "$ROMSET_PSP_REDUMP_URL",
      "resources": {
        "dat": {
          "source": "http://redump.org/datfile/psp/",
          "download": "$HOME/retrokit/tmp/psp/roms.zip",
          "target": "$HOME/retrokit/tmp/psp/roms.dat",
          "install": {"action": "zip_extract", "file": ".*\\.dat"}
        },
        "machine": {
          "source": "{url}{page_number}/{machine}.zip",
          "download": "$HOME/RetroPie/roms/psp/.redump/{machine}.zip",
          "target": "$HOME/RetroPie/roms/psp/.redump/{machine}.cso",
          "install": {"action": "zip_to_cso"}
        }
      }
    }
  ],
  "metadata": {
    "parent": {
      "source": "https://github.com/unexpectedpanda/retool/raw/main/clonelists/Sony%20-%20PlayStation%20Portable%20(Redump).json",
      "target": "$HOME/retrokit/tmp/psp/clones.json"
    },
    "scraper": {
      "source": "file://$HOME/retrokit/cache/scraper/psp.json"
    }
  },
  "roms": {
    "priority": [
      "USA"
    ],
    "filters": {
      "flags": [
        "USA"
      ],
      "categories": [
        "Games"
      ],
      "!keywords": [
        "Cards",
        "Collection",
        "Demo",
        "Poker",
        "Puzzle",
        "Quiz",
        "# Sports franchises",
        "FIFA Soccer",
        "Gretzky NHL",
        "Madden NFL",
        "MLB 0",
        "MLB 1",
        "Major League Baseball",
        "NBA 0",
        "NBA 1",
        "NBA 2",
        "NBA Live",
        "NCAA Football",
        "PES 20",
        "Tiger Woods PGA Tour",
        "World Tour Soccer",
        "WWE SmackDown vs. Raw"
      ],
      "!titles": [
        "# Collections",
        "2 Games in 1! Archer Maclean's Mercury & Mercury Meltdown",
        "Activision Hits Remixed",
        "Atari Classics Evolved",
        "SNK Arcade Classics Vol. 1",
        "Sampler Disc for PSP Vol. 1",
        "Stealth + WipEout Pure",
        "# Board Games",
        "Chessmaster - The Art of Learning",
        "Online Chess Kingdoms",
        "Scrabble",
        "# Education",
        "LittleBigPlanet",
        "PQ - Practical Intelligence Quotient",
        "PQ2 - Practical Intelligence Quotient 2",
        "Spelling Challenges and More!",
        "# Non-Games",
        "Marvel Trading Card Game",
        "Neverland Card Battles",
        "Traxxpad - Portable Studio"
      ],
      "+titles": [
        "# Latest Sports Franchises (one day make this automatic)",
        "FIFA Soccer 13",
        "Gretzky NHL 06",
        "Madden NFL 12",
        "MLB 11 - The Show",
        "Major League Baseball 2K12",
        "NBA 2K13",
        "NBA Live 10",
        "NCAA Football 10",
        "PES 2014 - Pro Evolution Soccer",
        "Tiger Woods PGA Tour 10",
        "World Tour Soccer 06",
        "WWE SmackDown vs. Raw 2011"
      ],
      "demo": [
        "Simpsons Game, The"
      ]
    },
    "favorites": {
      "titles": [
      ]
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/psp"}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
