{
  "system": "snes",
  "overlays": {
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-SNES",
        "path": "retroarch/overlay/GameBezels/SNES"
      },
      {
        "repo": "thebezelproject/bezelprojectsa-SNES",
        "path": "retroarch/overlay/GameBezels/SNES",
        "default": "retroarch/overlay/Super-Nintendo-Entertainment-System.png"
      }
    ],
    "lightgun_border": {
      "offset_x": 223
    }
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "lr-snes9x": {"core_name": "snes9x", "library_name": "Snes9x", "default": true}
  },
  "cheats": {
    "name": "Nintendo - Super Nintendo Entertainment System"
  },
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Nintendo - Super Nintendo Entertainment System \\([0-9]"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$HOME/retrokit/cache/nointro/Nintendo - Super Nintendo Entertainment System (Parent-Clone).dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/snes/.nointro/{machine}.zip"
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "priority": [
      "USA",
      "World"
    ],
    "filters": {
      "flags": [
        "Europe",
        "USA",
        "World"
      ],
      "!flags": [
        "Aftermarket",
        "Alt",
        "Beta",
        "Collection",
        "Demo",
        "Enhancement",
        "Pirate",
        "Proto",
        "Sample",
        "Test",
        "Unl",
        "Virtual Console"
      ],
      "!keywords": [
        "Anniversary",
        "Cartridge",
        "Collector",
        "Gamecube",
        "Teenage Mutant Hero",
        "Tester"
      ],
      "!titles": [
        "# Collections",
        "Arcade's Greatest Hits - The Atari Collection 1",
        "Exertainment Mountain Bike Rally + Speed Racer",
        "# Board Games",
        "Chessmaster, The",
        "# Educational",
        "Miracle Piano Teaching System, The",
        "# Non-Games",
        "Super Game Boy",
        "XBAND"
      ],
      "demo": [
        "NBA Jam"
      ]
    },
    "favorites": {
      "titles": [
      ]
    },
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/snes/-ALL-"}
    ]
  }
}
