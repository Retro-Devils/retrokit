{
  "system": "snes",
  "overlays": {
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-SNES",
        "path": "retroarch/overlay/GameBezels/SNES"
      },
      {
        "repo": "thebezelproject/bezelprojectsa-SNES",
        "path": "retroarch/overlay/GameBezels/SNES",
        "default": "retroarch/overlay/Super-Nintendo-Entertainment-System.png"
      }
    ]
  },
  "scraper": {
    "sources": ["screenscraper"]
  },
  "emulators": {
    "lr-snes9x": {"core_name": "snes9x", "library_name": "Snes9x", "default": true}
  },
  "cheats": "Nintendo - Super Nintendo Entertainment System",
  "romsets": [
    {
      "name": "nointro",
      "protocol": "http",
      "discovery": {
        "type": "nointro",
        "base": "$ROMSET_NOINTRO_URL",
        "metadata": "{base}/no-intro_romsets_files.xml",
        "paths": {
          "url": "no-intro romsets/Nintendo - Super Nintendo Entertainment System \\([0-9]"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$HOME/retrokit/cache/nointro/Nintendo - Super Nintendo Entertainment System (Parent-Clone).dat"
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/snes/.nointro/{machine}.zip"
        }
      },
      "auth": "internetarchive"
    }
  ],
  "roms": {
    "priority": [
      "USA",
      "World"
    ],
    "filters": {
      "flags": [
        "Europe",
        "USA",
        "World"
      ],
      "!flags": [
        "aftermarket",
        "alt",
        "beta",
        "collection",
        "demo",
        "enhancement",
        "pirate",
        "proto",
        "sample",
        "test",
        "unl",
        "virtual console"
      ],
      "!keywords": [
        "anniversary",
        "cartridge",
        "collector",
        "gamecube",
        "teenage mutant hero",
        "tester"
      ],
      "!titles": [
        "Super Game Boy",
        "Tuff E Nuff ~ Fighting Spirit - Tuff E Nuff",
        "Wolfenstein 3D",
        "XBAND"
      ]
    },
    "favorites": {
      "titles": [
        "Aladdin",
        "Arkanoid - Doh It Again",
        "Contra III - The Alien Wars",
        "Donkey Kong Country",
        "Donkey Kong Country 2 - Diddy's Kong Quest",
        "Donkey Kong Country 3 - Dixie Kong's Double Trouble!",
        "Double Dragon V - The Shadow Falls",
        "Family Feud",
        "Final Fight",
        "Final Fight 2",
        "Final Fight 3",
        "Frogger",
        "George Foreman's KO Boxing",
        "Ken Griffey Jr. Presents Major League Baseball",
        "Lion King, The",
        "Madden NFL '94",
        "Madden NFL 97",
        "Marvel Super Heroes in War of the Gems",
        "Mortal Kombat",
        "Mortal Kombat 3",
        "Mortal Kombat II",
        "Ms. Pac-Man",
        "NBA Jam",
        "NBA Jam - Tournament Edition",
        "NHL '94",
        "NHL 97",
        "Olympic Summer Games",
        "Simpsons, The - Bart's Nightmare",
        "Space Invaders",
        "Spider-Man",
        "Spider-Man - Venom - Maximum Carnage",
        "Street Fighter II",
        "Street Fighter II Turbo",
        "Super Double Dragon",
        "Super Mario All-Stars",
        "Super Mario Kart",
        "Super Mario World",
        "Super Mario World 2 - Yoshi's Island",
        "Super Punch-Out!!",
        "Super Star Wars",
        "Super Star Wars - Return of the Jedi",
        "Super Star Wars - The Empire Strikes Back",
        "Super Tennis",
        "Teenage Mutant Ninja Turtles - Tournament Fighters",
        "Teenage Mutant Ninja Turtles IV - Turtles in Time",
        "Teenage Mutant Ninja Turtles IV - Turtles in Time",
        "Top Gear",
        "Top Gear 2",
        "Ultimate Mortal Kombat 3",
        "Worms",
        "X-Men - Mutant Apocalypse"
      ]
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/snes/-ALL-"},
      {
        "path": "$HOME/RetroPie/roms/snes",
        "filters": {"favorites": [true]}
      }
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
